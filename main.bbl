  \entry{zuber_dynamics_1961}{article}{}
    \name{author}{1}{}{%
      {{hash=ZN}{%
         family={Zuber},
         familyi={Z\bibinitperiod},
         given={Novak},
         giveni={N\bibinitperiod},
      }}%
