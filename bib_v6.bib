@article{chen_measurement_2020,
	title = {Measurement of the microlayer characteristics in the whole range of nucleate boiling for water by laser interferometry},
	volume = {146},
	issn = {0017-9310},
	url = {https://www.sciencedirect.com/science/article/pii/S0017931019342656},
	doi = {10.1016/j.ijheatmasstransfer.2019.118856},
	abstract = {During nucleate boiling, a thin liquid film (microlayer) is formed beneath a boiling bubble when the bubble undergoes rapid growth/expansion. The linear distribution of the microlayer has been previously confirmed, and its crest shape has been observed in the isolated bubble region of nucleate boiling. However, the microlayer behavior in larger heat flux regions up to critical heat flux has not yet been elucidated. In this study, to further understand the microlayer structure in the whole heat flux range of nucleate boiling, microlayer configuration was measured using laser interferometry. Water was adopted as the test fluid, and it is confirmed that the microlayer can be observed over a whole range of nucleate boiling containing the critical heat flux point. It is also confirmed that the deformation of the microlayer from axisymmetric shape was caused by complicated, irregular bubble motions such as bubble coalescence, which was observed for relatively higher heat flux. The crest shape of the microlayer, which appears near the periphery of its maximum diameter under relatively smaller heat flux, was not observed at a relatively higher heat flux. Finally, it is confirmed that heat flux does not obviously influence the thickness distribution of the initial microlayer.},
	pages = {118856},
	journaltitle = {International Journal of Heat and Mass Transfer},
	shortjournal = {International Journal of Heat and Mass Transfer},
	author = {Chen, Zhihao and Hu, Xiaocheng and Hu, Kang and Utaka, Yoshio and Mori, Shoji},
	urldate = {2022-10-10},
	date = {2020-01-01},
	langid = {english},
	keywords = {Water, High heat flux, Initial microlayer thickness, Laser interferometry, Microlayer structure, Nucleate pool boiling},
	file = {ScienceDirect Snapshot:C\:\\Users\\Luc\\Zotero\\storage\\AFZTH2NQ\\S0017931019342656.html:text/html},
}

@article{chen_detailed_2017,
	title = {Detailed structure of microlayer in nucleate pool boiling for water measured by laser interferometric method},
	volume = {108},
	issn = {0017-9310},
	url = {https://www.sciencedirect.com/science/article/pii/S0017931016328708},
	doi = {10.1016/j.ijheatmasstransfer.2017.01.003},
	abstract = {During nucleate pooling boiling, a microlayer forms on the heat transfer surface beneath a growing bubble. In previous studies, the existence of a microlayer was confirmed, and its structure was investigated by using several different experimental methods. However, the characteristics and mechanism of the formation of a microlayer have not been thoroughly elucidated to date. In this study, in order to obtain further understanding on the microlayer structure, a laser interferometric method was adopted for a more detailed observation. As a result, it was observed that the special bended shape of the microlayer forms near its outer margin when the microlayer radius is close to its maximum value. By analyzing the variation of the ratio of bubble height to diameter, it can be inferred that the detachment of a bubble causes a decrease of the initial microlayer thickness when the microlayer thickness is close to the maximum radius. This effect explains the formation of the bended shape of initial microlayer.},
	pages = {1285--1291},
	journaltitle = {International Journal of Heat and Mass Transfer},
