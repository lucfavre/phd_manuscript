@thesis{manon_contribution_2000,
	title = {Contribution à l'analyse et à la modélisation locale des écoulements bouillants sous-saturés dans les conditions des réacteurs à eau sous pression},
	rights = {Licence Etalab},
	url = {https://theses.fr/2000ECAP0696},
	abstract = {Ce travail est une contribution à la compréhension et à la modélisation de l'ébullition sous-saturée en convection forcée d'un fluide, le fréon 12, simulant des conditions d'écoulement rencontrés dans les réacteurs à eau sous pression ({REP}). La première partie porte sur l'analyse des résultats expérimentaux obtenus sur la boucle {DEBORA} du {CEA}/Grenoble. Une étude bibliographique critique des modèles monodimensionnels de taux de vide, de température de paroi, de pertes de pression et la comparaison de ces modèles aux résultats expérimentaux ont permis d'établir un ensemble de modèles applicables à la modélisation monodimensionnelle des écoulements bouillants sous-saturés d'eau et de fréon 12. La deuxième partie est consacrée à une analyse multidimensionnelle réalisée grâce à des simulations numériques comparées aux résultats expérimentaux. Ces simulations donnent de bons résultats malgré de nombreuses hypothèses simplificatrices. Cherchant à améliorer la modélisation de l'ébullition en paroi et à la décomposition du flux transmis de la paroi vers le fluide, on met en évidence les lacunes qui existent à ce jour et les travaux qu'il serait intéressant d'entreprendre. Par ailleurs, on s'intéresse au diamètre des bulles qui est impose et uniforme dans le code de calcul alors que les résultats expérimentaux montrent clairement que ce diamètre varie fortement le long de l'écoulement. On montre cependant que cette hypothèse de diamètre uniforme donne des résultats satisfaisants bien qu'une prédiction plus précise de cette grandeur pourrait probablement permettre une amélioration quantitative des résultats. Cela nous a amnés à étudier les possibilités d'introduction d'une équation de transport d'aire interraciale dans les écoulements bouillants afin de mieux décrire l'évolution du diamètre des bulles. Cette étude montre qu'à ce jour il manque encore des informations au niveau local, et que des études expérimentales doivent être menées notamment pour mieux appréhender les phénomènes de nucléation en paroi.},
	institution = {Châtenay-Malabry, Ecole centrale de Paris},
	type = {These de doctorat},
	author = {Manon, Etienne},
	editora = {Delhaye, Jean-Marc},
	editoratype = {collaborator},
	urldate = {2022-11-03},
	date = {2000-01-01},
	keywords = {Ébullition, Réacteurs à eau sous pression, Convection forcée, Modèle 1 dimension, Modèle N dimensions},
}

