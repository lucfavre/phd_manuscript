	langid = {english},
	note = {Publisher: Cambridge University Press},
	keywords = {boiling, contact lines, multiphase flow},
	file = {Full Text PDF:C\:\\Users\\Luc\\Zotero\\storage\\Y87QPB7G\\Bureš et Sato - 2021 - On the modelling of the transition between contact.pdf:application/pdf;Snapshot:C\:\\Users\\Luc\\Zotero\\storage\\IQ3UQMAP\\05713699070C2B3DE6F9BB36E5E0ADF3.html:text/html},
}

@article{urbano_direct_2018,
	title = {Direct numerical simulation of nucleate boiling in micro-layer regime},
	volume = {123},
	issn = {0017-9310},
	url = {https://www.sciencedirect.com/science/article/pii/S0017931017353619},
	doi = {10.1016/j.ijheatmasstransfer.2018.02.104},
	abstract = {The physical mechanisms associated with the evolution of a micro-layer beneath a bubble and the transition between contact line and micro-layer regimes are investigated with fully resolved numerical simulations, in the framework of nucleate pool boiling. Capturing the transition between these two regimes has been possible for the first time using very refined grids and parallel computations. Indeed, grids with a cell size under 1 μm must be used in order to capture thermal and dynamical effects in the micro-layer. Such multiscale computations require advanced code capabilities. The present simulations are used to analyse the physical processes involved in the formation and depletion of a micro-layer. A parametric study is carried out to investigate the impact of the main parameters affecting the presence of the micro-layer. From these results, the limit conditions between nucleate boiling in micro-layer and contact line regimes are deduced. Neglecting the micro-layer would lead to erroneous results because it has a strong influence on the overall bubble growth. Therefore the present results could be of major interest for designing models of nucleate pool boiling in larger scales computations, when the micro-layer cannot be resolved.},
	pages = {1128--1137},
	journaltitle = {International Journal of Heat and Mass Transfer},
	shortjournal = {International Journal of Heat and Mass Transfer},
	author = {Urbano, A. and Tanguy, S. and Huber, G. and Colin, C.},
	urldate = {2022-08-02},
	date = {2018-08-01},
	langid = {english},
	keywords = {Direct numerical simulation, Micro-layer, Nucleate boiling},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Luc\\Zotero\\storage\\H8FW2ZH7\\Urbano et al. - 2018 - Direct numerical simulation of nucleate boiling in.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Luc\\Zotero\\storage\\Z962PXMP\\S0017931017353619.html:text/html},
}

